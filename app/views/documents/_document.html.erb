<div id="<%= dom_id document %>">
  <p>
    <strong>User:</strong>
    <%= document.user_id %>
  </p>

  <p>
    <strong>File:</strong>
    <%= link_to document.file.filename, document.file if document.file.attached? %>
  </p>

  <p>
    <strong>Processed:</strong>
    <%= document.processed %>
  </p>

</div>
